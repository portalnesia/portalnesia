{
  "name": "@portalnesia/portalnesia-js",
  "version": "1.0.0",
  "description": "Official javascript wrapper for the Portalnesia API",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./server": "./dist/server.js",
    "./blog": "./dist/api/blog/index.js"
  },
  "typesVersions": {
    "*":{
      "server":["./dist/server.d.ts"],
      "blog":["./dist/api/blog/index.js"]
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "npx rimraf ./dist",
    "build": "npx ttsc",
    "bundle": "npx webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/portalnesia/portalnesia.git"
  },
  "typedoc": {
    "entryPoint": "src/index.ts",
    "displayName": "Portalnesia NodeJs SDK"
  },
  "keywords": [
    "api",
    "wrapper",
    "axios",
    "portalnesia",
    "rest",
    "api"
  ],
  "author": "Putu Aditya <aditya@portalnesia.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/portalnesia/portalnesia/issues"
  },
  "homepage": "https://portalnesia.com",
  "dependencies": {
    "axios": "^0.25.0",
    "idtoken-verifier": "^2.2.2",
    "pkce-challenge": "^2.2.0",
    "qs": "^6.10.3",
    "simple-oauth2": "^4.3.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.13",
    "@types/qs": "^6.9.7",
    "@types/simple-oauth2": "^4.1.1",
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "https-browserify": "^1.0.0",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "rimraf": "^3.0.2",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^9.2.6",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "ttypescript": "^1.5.13",
    "typescript": "^4.5.5",
    "typescript-transform-paths": "^3.3.1",
    "util": "^0.12.4",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  }
}
