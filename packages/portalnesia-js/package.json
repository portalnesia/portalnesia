{
  "name": "@portalnesia/portalnesia-js",
  "version": "1.0.0",
  "description": "Official javascript wrapper for the Portalnesia API",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./blog": "./dist/blog/*",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "npx rimraf ./dist",
    "version": "npx genversion --es6 src/version.ts",
    "rollup": "npx rollup -c",
    "build": "npx tsc --module commonjs --target esnext --outDir dist",
    "build:dev":"npx tsc --watch --module commonjs --target esnext --outDir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/portalnesia/portalnesia.git"
  },
  "keywords": [
    "api",
    "wrapper",
    "axios",
    "portalnesia",
    "rest",
    "api"
  ],
  "author": "Putu Aditya <aditya@portalnesia.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/portalnesia/portalnesia/issues"
  },
  "homepage": "https://portalnesia.com",
  "dependencies": {
    "encoding": "^0.1.13",
    "idtoken-verifier": "^2.2.2",
    "isomorphic-unfetch": "^3.1.0",
    "pkce-challenge": "^2.2.0",
    "qs": "^6.10.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/node": "^17.0.13",
    "@types/qs": "^6.9.7",
    "@types/simple-oauth2": "^4.1.1",
    "genversion": "^3.0.2",
    "patch-package": "^6.4.7",
    "pkg-dir": "^6.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "terser": "^5.12.1",
    "typescript": "^4.5.5"
  }
}
